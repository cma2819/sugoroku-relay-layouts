{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "startAtTimestamp": { "type": "number" },
    "endAtTimestamp": { "type": "number" },
    "timeInSeconds": { "type": "number" },
    "timeDisplayText": {
      "type": "object",
      "properties": {
        "up": { "type": "string" },
        "down": { "type": "string" }
      },
      "additionalProperties": false,
      "required": ["up", "down"]
    }
  },
  "required": [
    "startAtTimestamp",
    "endAtTimestamp",
    "timeInSeconds",
    "timeDisplayText"
  ],
  "additionalProperties": false
}
